<h1><%= list.name %></h1>
<ul class="list-show"></ul>
<% @lists.each do |movie| %>
    <li><%= movie.title%></li>
<% end %>

<%= cl_image_tag list.photo.key, height: 300, width: 400, crop: :fill %>

<%= simple_form_for(@list) do |f| %>
  <%= f.input :name %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
    <%= f.submit %>
<% end %>
